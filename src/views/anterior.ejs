<% include partials/_header.ejs %>
<!--FORM-->
<div class="col-md-5">
    <div class="card">
      <div class="card-body">
        <form action="/reporteanterior" method="get">
          <div class="form-group">
            <input class="form-control" type="number" name="año" placeholder="Año">
          </div>
          <div class="form-group">
            <label class="col-sm-12">Seleccionar Mes</label>
            <div class="col-sm-12">
                <select class="form-control form-control-line" name="mes" type="number">
                    <option value=1>Enero</option>
                    <option value=2>Febrero</option>
                    <option value=3>Marzo</option>
                    <option value=4>Abril</option>
                    <option value=5>Mayo</option>
                    <option value=6>Junio</option>
                    <option value=7>Julio</option>
                    <option value=8>Agosto</option>
                    <option value=9>Septiembre</option>
                    <option value=10>Octubre</option>
                    <option value=11>Noviembre</option>
                    <option value=12>Diciembre</option>
                </select>
            </div>
        </div>
          <button class="btn btn-primary" type="submit">
            Enviar
          </button>
        </form>
      </div>
    </div>
  </div>
  <% include partials/_footer.ejs %>